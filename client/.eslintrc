{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react",
        "plugin:react/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "no-console": "off",
        "no-param-reassign": "off",
        "no-use-before-define": "off",
        "react/prop-types": 1,
        "react/forbid-prop-types": 0,
        "no-underscore-dangle": 0,
        "sort-keys": "warn",
        "sort-vars": "warn",
        "import/imports-first": [
            "error",
            "absolute-first"
        ],
        "import/newline-after-import": "error",
        "eofline": true,
        "singleQuote": true
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "FormData": true,
        "FileReader": true,
        "Blob": true,
        "navigator": true
    },
    "parser": "babel-eslint"
}